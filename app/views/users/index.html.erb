<div class="container-fluid">
  <div class="row-fluid">
    <h1><%= @users.count %> Active users</h1><br/>
    <%= link_to "Banned Users", banned_users_path , class: 'btn btn-success' %>
  </div>

  <div class="tab-pane push-down">
    <table class="table table-hover">
      <%= render partial: 'heading' %>
      <%= render partial: 'user', collection: @users %>
    </table>
  </div>
  <div class="paginate">
    <%= will_paginate @users %>
  </div>
</div>