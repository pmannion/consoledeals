<%= content_for :title do %>New product<% end %>

<div class="container" id="generic">
  <div class="row-fluid">
    <div class="span12">
    <h1 id ="h1-override"><% if @product %>You are editing <%= @product.name %>
          <% else %>You are creating a new product<% end %></h1>
    <div class="hero-unit">
      <div class="errors">


      </div>
      <div class="row-fluid">
        <%= simple_form_for (@product), url: "/admin/products/create/"  do |f| %>
            <%= render partial: 'newedit', locals: { :f => f} %>
        <% end %>
      </div>
    </div>
  </div>
</div>
</div>

