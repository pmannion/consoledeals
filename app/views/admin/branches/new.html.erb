    <h1 id ="h1-override">You are creating a new branch</h1>
    <div class="hero-unit">
      <div class="errors">

      </div>
      <div class="row-fluid">
        <%= simple_form_for (@branch), url: "/admin/branches/create/"  do |f| %>
            <%= render partial: 'newedit', locals: { :f => f} %>
        <% end %>
      </div>
    </div>
  </div>
</div>


