<div class="container" id="generic">
  <div class="row-fluid">
    <h1 id="h1-override">You are editing <%= @branch.name %></h1>
    <div class="hero-unit">
      <div class="errors">

      </div>
      <div class="row-fluid">
        <%= simple_form_for (@branch), url: update_branch_path(@branch)  do |f| %>
            <%= render :partial => 'newedit', :locals => { :f => f}  %>
        <% end %>
      </div>
    </div>
  </div>
</div>
