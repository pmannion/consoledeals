<div class="container" id="generic">
  <div class="row-fluid">
    <h1><%= @branches.size if @branches %> Active Branch</h1>
      <div class="span12">
        <%= link_to "Create Branch" , create_branch_path, class: "btn btn-large btn-success"%>
        <%= link_to "Inactive Branches" , inactive_branches_path, class: "btn btn-large btn-success"%>
      </div>

    <div class="tab-pane push-down">
      <table class="table">
        <tbody>
        <%= render :partial => 'heading' %>
        <%= render :partial => 'branch', :collection => @branches %>
        </tbody>
      </table>
      </div>
      <div class="paginate">
        <%= will_paginate @branches %>
      </div>
    </div>
</div>



